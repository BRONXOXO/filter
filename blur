// Blur image
void blur(int height, int width, RGBTRIPLE image[height][width])
{
    RGBTRIPLE new_image[height][width];
    for(int i = 0; i < height; i++)
    {
       for(int j = 0; j < width; j++)
       {
           int sumRed = 0;
           int sumGreen = 0;
           int sumBlue = 0;
           int noofnewpixels = 0;

           for (int k = -1; k < 2; k++)
            {
                if( k + i < 0 || k + i > height - 1)
                {
                    continue;
                }

                for (int l = -1; l < 2; l++)
                {
                    if( l + j < 0 || l + j > width - 1)
                    {
                        continue;
                    }

                    int new_i = k + i;
                    int new_j = l + j;

                    noofnewpixels++;
                    new_image[i][j].rgbtRed = round((float) new_image[new_i][new_j].rgbtRed / noofnewpixels );
                    new_image[i][j].rgbtGreen = round((float) new_image[new_i][new_j].rgbtGreen / noofnewpixels );
                    new_image[i][j].rgbtBlue = round((float) new_image[new_i][new_j].rgbtBlue / noofnewpixels );
                    
                    image[i][j].rgbtBlue = new_image[i][j].rgbtBlue;
                    image[i][j].rgbtGreen = new_image[i][j].rgbtGreen;
                    image[i][j].rgbtRed = new_image[i][j].rgbtRed;
                    
                    printf("%i", image[i][j].rgbtBlue);

                }
            }
       }

    }
}
